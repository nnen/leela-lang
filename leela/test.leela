
var makeCounter, counter, obj;
begin
	makeCounter := lambda (countFrom) : function () :
	var result;
	begin
		result := countFrom;
		countFrom := countFrom - 1;
		return result
	end;
	
	counter := makeCounter(10);
	print "Counting from 10...";
	print counter();
	print counter();
	print counter();
	print counter();
	print counter();
	
	counter := makeCounter(3);
